window.Search = {
  initialize () {
    let initialSearchState = {
       ...gon.search,
       numGuests: parseInt(gon.search.numGuests),
       entireHome: gon.search.entireHome || false,
       privateRoom: gon.search.privateRoom || false,
       sharedRoom: gon.search.sharedRoom || false,

       minPrice: 10,
       maxPrice: 999
    };

    initializeSearchStateStore(initialSearchState);

    ReactDOM.render(
      <ReactRedux.Provider store={searchStateStore}>
        <SearchMain />
      </ReactRedux.Provider>,
      document.getElementById("search")
    );
  }
};
